#!/usr/bin/php
<?php

define('RIOTQUEST_ENV', 'CLI');

require __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;

use RiotQuest\Components\Console\Docs\MakeCommand;
use RiotQuest\Components\Console\Api\Summoner\NameCommand;

$app = new Application();

$app->add(new MakeCommand());
$app->add(new NameCommand());

$app->run();
